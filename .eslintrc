{
  "extends": ["prettier", "prettier/react"],
  "plugins": ["react", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier" : [
      "error",
      {
        "singleQuote": false,
        "semi": true,
        "endOfLine": "auto"
      }
    ],
    "react/prop-types": [1]
  }
}
